<h1>Posts Index</h1>



<table class="table table-striped table-hover">
				<thead>
					<tr>
						<th>
							#
						</th>
						<th>
							Date
						</th>
						<th>
							User
						</th>
						<th>
							Rationale
						</th>
					</tr>
				</thead>
				<tbody>
          <% @posts.each do |post| %>
					  <%= render partial: 'post', locals: { post: post } %>
          <% end %>
				</tbody>
			</table>
